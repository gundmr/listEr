    "url": "https://github.com/gundmr/listEr/issues"
  },
  "homepage": "https://github.com/gundmr/listEr#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^3.6.0",
    "config": "^3.0.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.13"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  }
